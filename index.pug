doctype html
header 
    link(href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous")
    link( rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css")
    link(rel="stylesheet", href="../css/index.css")
    title Tuyen dung
body 
    .header
        .container.d-flex.justify-content-between.align-items-center
            .container-logo.d-flex.align-items-center.justify-content-center
                <span>T</span>uyen<span>d</span>ung 
            .container-info.d-flex.d-flex.align-items-center.justify-content-center
                .notice
                    img(src="../notification-bell.png", alt="notice")
                .info.d-flex.align-items-center.justify-content-center
                    .box-img.d-flex.align-items-center.justify-content-center
                        img(src="../user-circle-solid-24.png", alt="user")
                    span.name Dao Phan Quoc Hoai
                .login.d-none
                    p Đăng nhập 
                .register.d-none 
                    p Đăng ký 
    .container 
        .vitri 
            .vt 
                input(type="text" name="position" placeholder="Vị trí tuyển dụng" style="border: none; width: 200px; height: 50px;")
            .khuvuc  
                select(name="khuvuc" id="khuvuc")
                    option(value="kvuc") Tất cả tỉnh/thành phố
                    option(value="TPHCM") TP Hồ Chí Minh
                    option(value="HN") Hà Nội
                    option(value="NT") Nha Trang
                    option(value="Hue") Huế
        .kinhnghiem 
            select(name="exp" id="exp")
                option(value="kn") Tất cả kinh nghiệm
                option(value="none") Chưa có kinh nghiệm
                option(value="duoi1nam") Dưới 1 năm
                option(value="1nam") 1 năm
                option(value="2nam") 2 năm
                option(value="3nam") 3 năm
                option(value="4nam") 4 năm
                option(value="5nam") 5 năm
                option(value="tren5nam") Trên 5 năm
        .luong 
            select(name="luong" id="luong")
                option(value="luong") Chọn mức lương
                option(value="duoi10") Dưới 10 triệu
                option(value="10den15") 10 - 15 triệu
                option(value="15den20") 15 - 20 triệu
                option(value="tren20") Trên 20 triệu
        .tim
        
            button(type='button') Tìm kiếm
    

